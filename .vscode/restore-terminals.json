{
    "artificialDelayMilliseconds": 700,
    "keepExistingTerminalsOpen": false,
    "runOnStartup": true,
    "terminals": [
        {
            "splitTerminals": [
                {
                    "name": "slicemachine",
                    "commands": ["npm run slicemachine"]
                },
                {
                    "name": "nextjs",
                    "commands": ["npm run dev"]
                },
                {
                    "name": "installs"
                },
                {
                    "name": "test",
                    "commands": ["jest --watch"]
                }
            ]
        }
    ]
}
